--transforms
tfms = {}

transform = {}
_transform_mt = {
 __index=transform
 }
 
function _transform_mt.__call(t,x,y)
 return (x-64)*t.sx+t.tx,
        (y-64)*t.sy+t.ty
end

function _transform_mt.__tostring(t)
 return "transform("
   ..t.tx .. "," .. t.ty .. ","
   ..t.sx .. "," .. t.sy .. ")"
end

function transform.new(tx,ty,sx,sy)
 o = {tx=tx,ty=ty,sx=sx,sy=sy}
 setmetatable(o,_transform_mt)
 return o
end

function transform.copy(t)
 return transform.new(t.tx,t.ty,t.sx,t.sy)
end

function transform.current()
 return tfms[#tfms]
end

function transform.push(t)
 add(tfms, t)
end

function transform.pop()
 if #tfms > 1 then
  return pop(tfms)
 end
 return nil
end
