-- mandelbrot
iterations=48
threshold=10000

function f(x, y, a, b)
 return a*a-b*b + x, 2*a*b + y 
end
function mag2(x, y)
 return x*x + y*y
end

function val(x,y)
 local za, zb = 0, 0
 for n=1,iterations do
  za,zb = f(x,y,za,zb)
  if mag2(za,zb) > threshold then
   return n
  end
 end
 return 0
end
