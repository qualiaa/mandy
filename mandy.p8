-- main loop

dragging = false

function _init()
 poke(0x5f2d, 1)
 mx, my = stat(32), stat(33)
 push_tfm(transform.new(
  0,0,1/32,1/32))
  
 menuitem(1,"previous view",previous_tfm)
 menuitem(2,"equalise scale",equalise_scale) 
end

function _draw()
 local t = transform.current()
 if redraw then
  for i=0,127 do
   for j=0,127 do
    draw_m(i,j,t)
   end
  end
  redraw=false
 end
end

function _update()
 lmx, lmy = mx, my
 mx, my = stat(32), stat(33)
 local t = transform.current()
 local dx, dy = 8*t.sx, 8*t.sy
 t = t:copy()
 local change=false
 if btnp(0) then t.tx-=dx;change=true end
 if btnp(1) then t.tx+=dx;change=true end
 if btnp(2) then t.ty-=dy;change=true end
 if btnp(3) then t.ty+=dy;change=true end
 if btnp(4) then t.sx/=2;t.sy/=2;change=true end
 if btnp(5) then t.sx*=2;t.sy*=2;change=true end
 if change then
  push_tfm(t)
 end
 
 mouse1 = (stat(34) & 1) ~= 0
 mouse2 = (stat(34) & 2) ~= 0
 if mouse1 then
  if not dragging then
   last_w = nil
   current_w = window.new(mx,my,mx,my)
  else
   last_w = current_w
   current_w = last_w:reshape(mx,my)
  end
  dragging=true
 elseif dragging then
   push_tfm(
    current_w:to_transform())
   dragging=false
 elseif mouse2 then
  previous_tfm()
 end
 
 -- this needs to happen in
 -- _update as _draw may be 
 -- skipped
 if dragging then
  if last_w then
   redraw_last(t)
  end
  current_w:draw()
 else
  draw_m(lmx,lmy,t)
  pset(mx,my,1)
 end
end

function previous_tfm()
 if transform.pop() then
  redraw=true
 end
end

function push_tfm(t)
 transform.push(t)
 redraw=true
end

function equalise_scale()
 local t = transform.current():copy()
 local s = (t.sx+t.sy)/2
 t.sx, t.sy = s, s
 push_tfm(t)
end
