-- window
window = {}

_window_mt={__index=window}

function window.new(sx,sy,ex,ey)
 local w = {
  start={sx,sy},
  finish={ex,ey}
 }
 setmetatable(w, _window_mt)
 return w
end

function window.as_rect(w)
 local sx, sy = unpack(w.start)
 local ex, ey = unpack(w.finish)
 local x0, x1 = order(sx, ex)
 local y0, y1 = order(sy, ey)
 return x0,y0,x1,y1
end

function window.as_rect_sp(w)
 local x0,y0,x1,y1 = w:as_rect()
 return x0-0.5,y0-0.5,x1+0.5,y1+0.5
end

function window.draw(w)
 fillp(0b0101101001011010)
 local sx, sy = unpack(w.start)
 local ex, ey = unpack(w.finish)
 local x0, x1 = min(sx,ex), max(sx,ex)
 local y0, y1 = min(sy,ey), max(sy,ey)
 rect(x0,y0,x1,y1,7+(13<<4))
 fillp()
end

function window.reshape(w,ex,ey)
 local sx, sy =unpack(w.start)
 return window.new(sx,sy,ex,ey)
end

function window.to_transform(w)
 local px0,py0,px1,py1 = w:as_rect_sp()
 local t = transform.current()
 local x0,y0 = t(px0,py0)
 local x1,y1 = t(px1,py1)
 
 return transform.new(
  (x1+x0)/2,
  (y1+y0)/2,
  (x1-x0)/128,
  (y1-y0)/128
 )
end

function calc_redraw_lengths(new_w, old_w)
 local x1,y1=unpack(old_w.finish)
 local x2,y2=unpack(new_w.finish)

 if x1==x2 and y1==y2 then
  return nil,nil,nil,nil
 end
 
 local x0,y0=unpack(old_w.start)
 
 local dx0=mid(x0,x1,x2) - x1
 local dy0=mid(y0,y1,y2) - y1
 local dx1=dx0
 local dy1=dy0
 
 if x1~=x2 then
  dy1 = y0-y1
 end
 if y1~=y2 then
  dx1 = x0-x1
 end
 return 
  asc_line_seg(x1,x1+dx0),
  asc_line_seg(y1,y1+dy0),
  asc_line_seg(x1,x1+dx1),
  asc_line_seg(y1,y1+dy1)
end

function asc_line_seg(x0,x1)
 if x0==x1 then
  return nil
 end
 return ordert(x0,x1)
end
